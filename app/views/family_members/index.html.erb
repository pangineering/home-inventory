<p style="color: green"><%= notice %></p>

<h1>Family members</h1>

<%= link_to "New family member", new_family_member_path, class: "btn btn-primary"  %>

<div id="family_members">
  <% @family_members.each do |family_member| %>

    <div style="margin-top: 1rem; border: 1px solid black; padding: 1.5rem; border-radius: 1.5rem">
    <%= render family_member %>
    <p>
      <%= link_to "Show", family_member, class: "btn btn-info"  %>
    </p>
    </div>
  <% end %>
</div>


